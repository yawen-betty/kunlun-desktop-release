{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Kunlun",
  "version": "1.0.0",
  "identifier": "com.yawen.kunlun",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist",
    "removeUnusedCommands": true
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "昆仑",
        "maximized": true,
        "minWidth": 1366,
        "minHeight": 768,
        "decorations": false,
        "resizable": true,
        "transparent": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDVDQUYyMzg2ODkxMDgzQjkKUldTNWd4Q0poaU92WEQrdzlWODBhaUdnYXJLdUZhc2pHbWw5MXBLMlNSTHRwRTN1ZGNMZ0pwZG4K",
      "endpoints": [
        "http://update.liangjiyuren.cn/kunlun/{{target}}/latest.json"
      ]
    }
  }
}
